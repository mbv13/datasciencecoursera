corr <- function(directory, threshold = 0)#
{#
	path <- paste0(getwd(),"/",directory)#
	corr_vect <- NULL#
	for(i in 1:332)#
	{#
		if(i<10)#
		{#
			dat <- read.csv(paste(path,"/00",as.character(i),".csv",sep=""),as.is=T,header=T)#
		}#
		else if(i<100)#
		{#
			dat <- read.csv(paste(path,"/0",as.character(i),".csv",sep=""),as.is=T,header=T)#
		}#
		else#
		{#
			dat <- read.csv(paste(path,"/",as.character(i),".csv",sep=""),as.is=T,header=T)#
		}#
		data <- dat[complete.cases(dat),]#
		if(nrow(data) > threshold)#
		{#
			corr_vect <- c(corr_vect, cor(data[,"sulphate"],data[,"nitrate"])) #
		}#
	}#
	return(corr_vect)#
}
